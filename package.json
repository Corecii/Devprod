{
  "name": "devprod",
  "version": "0.4.2",
  "description": "A tool to set Roblox Developer Product configuaration from a json file",
  "keywords": [
    "roblox"
  ],
  "homepage": "https://github.com/Corecii/Devprod",
  "bugs": "https://github.com/Corecii/Devprod/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Corecii/Devprod.git"
  },
  "main": "./out/main.js",
  "bin": {
    "devprod": "./bin/cli"
  },
  "files": [
    "out/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "tsc",
    "binpack": "tsc && pkg -o devprod -t node10-win-x64,node10-win-x86,node10-macos-x64,node10-linux-x64 .\\out\\main.js",
    "mtest": "tsc && node out/main.js"
  },
  "author": "corecii@corecii.com",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/object-hash": "^1.3.0",
    "@types/winreg": "^1.2.30",
    "typescript": "^3.7.2",
    "tslint": "^5.20.1"
  },
  "dependencies": {
    "@iarna/toml": "https://github.com/Corecii/iarna-toml",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.45",
    "beautiful-dom": "^1.0.7",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "fs-extra": "^8.1.0",
    "object-hash": "^2.0.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "winreg": "^1.2.4"
  }
}
